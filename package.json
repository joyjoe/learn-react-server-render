{
  "name": "learnwebpack",
  "version": "1.0.0",
  "description": "webpack 3 in action",
  "main": "index.js",
  "scripts": {
    "build:client": "webpack --config webpack.config.client.js --colors --progress --display-reasons",
    "build:server": "webpack --config webpack.config.server.js --colors --progress --display-reasons",
    "build": "npm run clean && npm run build:client && npm run build:server",
    "dev:client": "cross-env NODE_ENV=development webpack-dev-server --config webpack.config.client.js --hotOnly",
    "dev:server:once": "cross-env NODE_ENV=development node ./server/server.js",
    "dev:server": "cross-env NODE_ENV=development nodemon ./server/server.js",
    "server:once": "node ./server/server.js",
    "server": "nodemon ./server/server.js",
    "clean": "rimraf dist",
    "eslint": "eslint --ext .js --ext .jsx src/",
    "eslint:fix": "eslint --ext .js --ext .jsx src/ --fix",
    "precommit": "npm run eslint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joyjoe/learn-webpack.git"
  },
  "keywords": [],
  "author": "joyjoe <zhoujiang@weathercn.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/joyjoe/learn-webpack/issues"
  },
  "homepage": "https://github.com/joyjoe/learn-webpack#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "clean-webpack-plugin": "^0.1.17",
    "cross-env": "^5.1.1",
    "css-loader": "^0.28.7",
    "eslint": "^4.13.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-react": "^7.5.1",
    "eslint-plugin-standard": "^3.0.1",
    "express": "^4.16.2",
    "file-loader": "^1.1.5",
    "html-webpack-plugin": "^2.30.1",
    "http-proxy-middleware": "^0.17.4",
    "husky": "^0.14.3",
    "memory-fs": "^0.4.1",
    "nodemon": "^1.12.5",
    "react-hot-loader": "^3.1.3",
    "rimraf": "^2.6.2",
    "style-loader": "^0.19.0",
    "uglifyjs-webpack-plugin": "^1.1.2",
    "url-loader": "^0.6.2",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.9.7"
  },
  "dependencies": {
    "axios": "^0.17.1",
    "babel-polyfill": "^6.26.0",
    "body-parser": "^1.18.2",
    "express-session": "^1.15.6",
    "mobx": "^3.4.1",
    "mobx-react": "^4.3.5",
    "node-fetch": "^1.7.3",
    "query-string": "^5.0.1",
    "react": "^16.2.0",
    "react-async-bootstrapper": "^1.1.2",
    "react-dom": "^16.2.0",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2"
  }
}
